<Page x:Class="WPF_Demo_1_Vorontsov_N_A_3802.Pages.PageShowAgents"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF_Demo_1_Vorontsov_N_A_3802.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageShowAgents">

    <Grid>
        <ScrollViewer>
            <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                <TextBox Width="200" Margin="20,20" HorizontalAlignment="Left" Height="25" Name="tbSearch"></TextBox>
                <ComboBox Name="cbSort" Width="150" Height="25"/>
                <ComboBox Name="cbFilter" Width="150" Margin="20,0" Height="25"/>
            </StackPanel>
            
        </ScrollViewer>
    </Grid>
</Page>
